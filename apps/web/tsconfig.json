{
  "extends": "@gw2treasures/tsconfig/nextjs",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/components/*": ["components/*"],
      "@/data/*": ["data/*"],
      "@/lib/*": ["lib/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/.well-known/**/*.ts",
    ".next/types/**/*.ts"
  ],
}
