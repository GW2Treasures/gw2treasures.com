{
  "name": "@gw2treasures/eslint-plugin-nextjs",
  "version": "0.1.2",
  "description": "ESLint plugin with custom rules for Next.js",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "lint": "eslint .",
    "publish-package": "gw2treasures-publish-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GW2Treasures/gw2treasures.com.git"
  },
  "keywords": [
    "gw2treasures",
    "eslint",
    "eslint-plugin",
    "nextjs",
    "metadata"
  ],
  "files": [
    "dist/"
  ],
  "author": "darthmaim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GW2Treasures/gw2treasures.com/issues"
  },
  "homepage": "https://github.com/GW2Treasures/gw2treasures.com#readme",
  "dependencies": {
    "@typescript-eslint/utils": "8.56.0"
  },
  "devDependencies": {
    "@gw2treasures/eslint-config": "workspace:*",
    "@gw2treasures/tsconfig": "workspace:*",
    "@types/node": "24.10.13",
    "eslint": "9.39.2",
    "tsdown": "0.20.3",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "eslint": "^8.56.0 || ^9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
