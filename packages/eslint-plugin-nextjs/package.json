{
  "name": "@gw2treasures/eslint-plugin-nextjs",
  "version": "0.1.0",
  "description": "ESLint plugin with custom rules for Next.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "pnpm run clean && tsup src",
    "lint": "eslint .",
    "clean": "rm -rf dist/",
    "publish-package": "gw2treasures-publish-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GW2Treasures/gw2treasures.com.git"
  },
  "keywords": [
    "gw2treasures",
    "eslint",
    "eslint-plugin",
    "nextjs",
    "metadata"
  ],
  "files": [
    "dist/"
  ],
  "author": "darthmaim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GW2Treasures/gw2treasures.com/issues"
  },
  "homepage": "https://github.com/GW2Treasures/gw2treasures.com#readme",
  "dependencies": {
    "@typescript-eslint/utils": "8.44.0"
  },
  "devDependencies": {
    "@gw2treasures/eslint-config": "workspace:*",
    "@gw2treasures/tsconfig": "workspace:*",
    "@types/node": "22.18.6",
    "eslint": "9.36.0",
    "tsup": "8.5.0",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "eslint": "^8.56.0 || ^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
